//export default new Vuex.Store({
    const store = new Vuex.Store({
    state: {
        clients: []
    },
    getters: {

    },
    actions: {
        getClients(context){
            context.commit('setClients')
        }
    },
    mutations: {
        setClients(state){
            //console.log(data);
            state.clients = data.getClients()
            console.log(data.getClients);
        }
    }
})

